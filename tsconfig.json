{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ES6", //"es6",
    "target": "es5", //"es6?"
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": true,
    //dependencia nova
    "esModuleInterop": true
  },
  "include": ["src/**/*.tsx", "src/**/*.ts"],
  "exclude": ["node_modules"]
}
